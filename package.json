		"express-session": "^1.17.2",
		"graphql": "^15.5.1",
		"ioredis": "^4.27.6",
		"module-alias": "^2.2.2",
		"pg": "^8.6.0",
		"reflect-metadata": "^0.1.13",
		"type-graphql": "^1.1.1",
		"typedi": "^0.10.0",
